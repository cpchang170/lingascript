{
  "name": "lingascript",
  "version": "0.1.0",
  "description": "Typescript with non-English languages",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "file-patch": "^0.2.1",
    "filecompare": "^1.0.4"
  },
  "devDependencies": {},
  "scripts": {
    "prebuild:checker": "./node_modules/.bin/file-patch src/common/compiler/checker.ts.diff typescript/src/compiler/checker.ts && ./node_modules/.bin/filecompare src/common/compiler/checker.ts typescript/src/compiler/checker.ts",
    "prebuild:scanner": "./node_modules/.bin/file-patch src/common/compiler/scanner.ts.diff typescript/src/compiler/scanner.ts && ./node_modules/.bin/filecompare src/common/compiler/scanner.ts typescript/src/compiler/scanner.ts",
    "prebuild:tsconfig": "./node_modules/.bin/file-patch src/common/compiler/tsconfig.json.diff typescript/src/compiler/tsconfig.json && ./node_modules/.bin/filecompare src/common/compiler/tsconfig.json typescript/src/compiler/tsconfig.json",
    "prebuild:types": "./node_modules/.bin/file-patch src/common/compiler/types.ts.diff typescript/src/compiler/types.ts && ./node_modules/.bin/filecompare src/common/compiler/types.ts typescript/src/compiler/types.ts",
    "prebuild": "npm run prebuild:checker && npm run prebuild:scanner && npm run prebuild:tsconfig && npm run prebuild:types",
    "build:cn": "cp src/cn/language.ts typescript/src/compiler/language.ts && npm run build",
    "build:tw": "echo 'Traditional Chinese build is not supported yet'",
    "build": "cd typescript && npm install && jake clean && jake local",
    "diff:checker": "./node_modules/.bin/file-diff typescript/src/compiler/checker.ts src/common/compiler/checker.ts && cp typescript/src/compiler/checker.ts.diff src/common/compiler/.",
    "diff:scanner": "./node_modules/.bin/file-diff typescript/src/compiler/scanner.ts src/common/compiler/scanner.ts && cp typescript/src/compiler/scanner.ts.diff src/common/compiler/.",
    "diff:tsconfig": "./node_modules/.bin/file-diff typescript/src/compiler/tsconfig.json src/common/compiler/tsconfig.json && cp typescript/src/compiler/tsconfig.json.diff src/common/compiler/.",
    "diff:types": "./node_modules/.bin/file-diff typescript/src/compiler/types.ts src/common/compiler/types.ts && cp typescript/src/compiler/types.ts.diff src/common/compiler/.",
    "diff": "npm run diff:checker && npm run diff:scanner && npm run diff:tsconfig && npm run diff:types",
    "install": "npm run update-ts",
    "pretest:en": "node typescript/built/local/tsc.js -p examples/en",
    "pretest:cn": "node typescript/built/local/tsc.js -p examples/cn",
    "test": "npm run pretest:en && npm run pretest:cn && node tests/test.js",
    "update-ts": "git submodule update --init --recursive",
    "upgrade-ts": "rm -Rf typescript && git submodule update --remote"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gasolin/lingascript.git"
  },
  "keywords": [
    "typescript",
    "non-english",
    "multilingal"
  ],
  "author": "Fred Lin <gasolin+npm@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gasolin/lingascript/issues"
  },
  "homepage": "https://github.com/gasolin/lingascript#readme"
}
